    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Trie Builder",
            "program": "${workspaceFolder}\\trie.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Odachi",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/app.ts",
            "cwd": "${workspaceFolder}",
            "protocol": "inspector",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}